@model IEnumerable<EURISTest.Models.ProductCatalog>
@{
    ViewBag.Title = "Index";
}
<h2>Index</h2>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            Catalogo
        </th>
        <th>
            Prodotto
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Catalog.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Product.Description)
            </td>
            <td>
                @Html.ActionLink("Delete", "DeleteProd", new { id = item.ProductCatalogID })
            </td>
        </tr>
    }
</table>
